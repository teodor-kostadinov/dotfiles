{
    "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
      "editor.defaultFormatter": "yzhang.markdown-all-in-one",
      "editor.quickSuggestions": {
        "comments": true,
        "other": true,
        "strings": true
      }
    },
    "[python]": {
      "editor.defaultFormatter": "ms-python.python",
      "editor.wordBasedSuggestions": false,
      "gitlens.codeLens.symbolScopes": ["!Module"]
    },
    "[typescript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[yaml]": {
      "editor.formatOnSave": false,
      "editor.defaultFormatter": "redhat.vscode-yaml",
      "editor.quickSuggestions": {
        "comments": false,
        "other": true,
        "strings": true
      },
      "editor.tabSize": 2
    },
    "yaml.format.bracketSpacing": false,
    "prettier.bracketSpacing": false,
    "cSpell.enableFiletypes": [
      "dockerfile",
      "makefile",
      "python",
      "shellscript",
      "terraform"
    ],
    "cSpell.userWords": [
      "abfs",
      "Adls",
      "autotermination",
      "azuread",
      "azurerm",
      "Balsamiq",
      "chdir",
      "databricks",
      "datalake",
      "dtap",
      "dynaconf",
      "getenv",
      "Hetzner",
      "hiyapyco",
      "Ingenii",
      "ipaddress",
      "jsondecode",
      "jupyterlab",
      "Kostadinov",
      "mgmt",
      "minutes",
      "munchify",
      "pconf",
      "privatelink",
      "pulumi",
      "recognised",
      "templatefile",
      "teodor",
      "vaultcore",
      "vnet",
      "wireframes",
      "WORKDIR",
      "yamldecode"
    ],
    "debug.console.fontSize": 13,
    "debug.onTaskErrors": "debugAnyway",
    "diffEditor.wordWrap": "off",
    "dotfiles.repository": "https://github.com/tksrc/dotfiles.git",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.bracketPairColorization.enabled": true,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": false,
    "editor.fontSize": 13,
    "editor.inlayHints.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "editor.quickSuggestions": true,
    "editor.quickSuggestionsDelay": 0,
    "editor.renderWhitespace": "all",
    "editor.suggestSelection": "first",
    "editor.wordWrap": "off",
    "editor.wordWrapColumn": 125,
    "explorer.compactFolders": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.exclude": {
      "**/node_modules": true
    },
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.ignoreMissingGitWarning": true,
    "github.copilot.enable": {
      "*": false,
      "markdown": false,
      "plaintext": false,
      "yaml": true
    },
    "githubIssues.queries": [
      {
        "label": "My Issues",
        "query": "default"
      },
      {
        "label": "Created Issues",
        "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
      }
    ],
    "gitlens.advanced.messages": {
      "suppressFileNotUnderSourceControlWarning": true,
      "suppressGitMissingWarning": true
    },
    "gitlens.blame.separateLines": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.mode.active": "zen",
    "gitlens.views.branches.files.layout": "tree",
    "markdown.preview.fontSize": 13,
    "notebook.cellToolbarLocation": {
      "default": "right",
      "jupyter-notebook": "left"
    },
    "omnisharp.useModernNet": true,
    "python.analysis.completeFunctionParens": true,
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": ["--max-line-length=130"],
    "python.linting.mypyArgs": [
      "--follow-imports=silent",
      "--show-column-numbers",
      "--strict"
    ],
    "python.linting.mypyEnabled": false,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
      "--disable=W",
      "--disable=C0116",
      "--disable=C0115",
      "--disable=C0114"
    ],
    "python.terminal.activateEnvInCurrentTerminal": true,
    "redhat.telemetry.enabled": false,
    "settingsSync.ignoredSettings": [
      "-dotfiles.repository",
      "-dotfiles.targetPath",
      "-dotfiles.installCommand",
      "-remote.containers.dotfiles.targetPath",
      "-remote.containers.dotfiles.repository",
      "-remote.containers.dotfiles.installCommand",
      "-remote.containers.copyGitConfig",
      "editor.fontSize",
      "terminal.integrated.fontSize"
    ],
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.scrollback": 9999,
    "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO"],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "vsicons.dontShowNewVersionMessage": true,
    "vsicons.presets.hideExplorerArrows": true,
    "workbench.colorCustomizations": {
      "editor.lineHighlightBackground": "#1073cf2d",
      "editor.lineHighlightBorder": "#9fced11f",
      "terminal.background": "#2f2833",
      "terminal.foreground": "#ffffff",
      "terminal.selectionBackground": "#2f00ff"
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.openMode": "singleClick",
    "workbench.startupEditor": "none",
    "explorer.autoReveal": false,
    "zentabs.maximumOpenedTabs": 4,
    "editor.minimap.enabled": false,
    "editor.accessibilitySupport": "off",
    "go.toolsManagement.autoUpdate": true,
    "settingsSync.ignoredExtensions": ["ms-dotnettools.csharp"],
    "githubPullRequests.createOnPublishBranch": "never",
    "emeraldwalk.runonsave": {
      "commands": [
        {
          "match": ".tf",
          "isAsync": true,
          "cmd": "terraform fmt --recursive"
        }
      ]
    },
    "json.schemas": [],
    "workbench.colorTheme": "Default Light+"
  }
  